{
  "name": "jptechlift",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && if [ -z \"$VERCEL\" ]; then npm run prerender; fi",
    "prerender": "tsx scripts/prerender.tsx",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint .",
    "test": "npm run lint && vitest run",
    "sync-deploy": "git push origin deploy && git push test deploy:main",
    "generate-sitemap": "node .sitemap-build/generate-sitemap.js",
    "postbuild": "node scripts/copy404.cjs && node scripts/generateRedirects.cjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@react-oauth/google": "^0.12.2",
    "aos": "^2.3.4",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "emailjs-com": "^3.2.0",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.3",
    "react-hook-form": "^7.62.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^9.1.0",
    "react-responsive": "^10.0.1",
    "react-router-dom": "^6.30.1",
    "swiper": "^11.2.10",
    "vite-plugin-pages-sitemap": "^1.7.1",
    "vite-plugin-robots": "^1.0.5",
    "vite-plugin-sitemap": "^0.8.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@rollup/wasm-node": "^4.46.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/aos": "^3.0.7",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "sass-embedded": "^1.89.2",
    "tailwindcss": "^3.4.4",
    "tailwindcss-cli": "^0.1.2",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "homepage": "https://baodhg.github.io/JPTechLift-WebApp",
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "eslint.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
